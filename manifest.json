{
    "name": "CrazyChat",
    "version": "0.0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "29",
    "icons": {
        "128": "images/icons/icon128.png"
    },
    "app": { 
        "background": {
            "scripts": ["scripts/background.js"]
        }
    },
    "permissions": [
        "http://*.uol.com.br/*",
        "http://*.avataruol.com.br/*",
        "notifications"
    ]
}
